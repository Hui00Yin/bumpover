{
  "name": "bumpover",
  "version": "0.3.2",
  "description": "ðŸš§ Controllable toolkit bumping data structure.",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn run build:es && yarn run build:cjs",
    "build:es": "rollup --config ./build/rollup.js",
    "build:cjs": "rollup --config ./build/rollup-cjs.js",
    "clean": "rm -rf ./lib ./node_modules",
    "example": "babel-node --presets env,stage-0",
    "example:basic": "yarn run example examples/basic",
    "example:xml": "yarn run example examples/xml",
    "lint": "standard",
    "release": "yarn run build && np",
    "test": "yarn run example:basic && yarn run example:xml",
    "watch": "rollup --config ./build/rollup.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doodlewind/bumpover.git"
  },
  "keywords": [
    "validation",
    "migration",
    "json",
    "xml"
  ],
  "author": "doodlewind",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doodlewind/bumpover/issues"
  },
  "homepage": "https://github.com/doodlewind/bumpover#readme",
  "standard": {
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "np": "^2.18.3",
    "rollup": "^0.53.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "standard": "^10.0.3",
    "superstruct": "^0.5.0",
    "xml-js": "^1.6.1"
  },
  "peerDependencies": {
    "superstruct": "^0.5.0"
  }
}
